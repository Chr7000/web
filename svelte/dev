#!/bin/sh
set -euo pipefail

rm -rf static/svelte
{
	echo "{# Generated by dev #}"
	echo '<script type="module" src="http://localhost:3000/@vite/client"></script>'
	echo '<script type="module" src="http://localhost:3000/src/GameDetail_init.js"></script>'
} > templates/svelte_include_generated.html

yarn run dev
